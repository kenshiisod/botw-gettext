msgid "Weapon_Sword_044_Name"
msgstr "[C9:00 bytes=\"02010100\"]Branche d'arbre"

msgid "Weapon_Sword_005_Name"
msgstr "[C9:00 bytes=\"02010100\"]Massue à pointes boko"

msgid "Weapon_Sword_007_Desc"
msgstr ""
"Cette épée à la lame tordue, très appréciée\n"
"des Lézalfos, peut être utilisée aussi bien au\n"
