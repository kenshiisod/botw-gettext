msgstr "[C9:00 bytes=\"02010100\"]Rebanadora"

msgid "Weapon_Sword_015_Name"
msgstr "[C9:00 bytes=\"02010100\"]Espada de guardián 3.0"

msgid "Weapon_Sword_017_Desc"
msgstr ""
