msgid "Com_Talk_11c"
msgstr "[Sound1 unk1=\"7\" unk2=\"11\"]Grazie, roccia!"

msgid "Com_Talk_11d"
msgstr "[Sound1 unk1=\"7\" unk2=\"9\"]Ti ringrazio."

