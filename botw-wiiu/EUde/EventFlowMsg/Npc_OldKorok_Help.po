msgid "talk01"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Oho! ♪ [PauseLong]Du willst immer noch plaudern?[Choice4Flags label1=\"0\" label2=\"1\" label3=\"2\" label4=\"3\" selectednum=\"0\" cancelnum=\"3\"]"

msgid "0001"
