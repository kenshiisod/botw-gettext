msgstr "[Sound1 unk1=\"11\" unk2=\"0\"]...Ja?[Choice2 label1=\"0\" label2=\"1\" selectednum=\"0\" cancelnum=\"1\"]"

msgid "talk15"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Was will Purah wohl damit machen...?"

msgid "talk03"
msgstr "[Sound1 unk1=\"0\" unk2=\"1\"]Hach..."

msgid "talk16"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]Diese [Color name=\"blue\"]Flamme[Color name=\"unset\"]...[PauseLong]\n"
"Früher hat man oft die Leute vom\n"
"[Color name=\"blue\"]Institut[Color name=\"unset\"] damit herumlaufen sehen."

msgid "talk04"
msgstr "[Sound1 unk1=\"0\" unk2=\"12\"]Hach... [PauseShort]Guten Tag."

msgid "talk17"
msgstr "[Sound1 unk1=\"0\" unk2=\"34\"]Ich schlafe. [PauseShort]Du gehst.[PauseShort] Ja?"

