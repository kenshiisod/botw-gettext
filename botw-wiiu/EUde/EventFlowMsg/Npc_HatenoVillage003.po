msgid "talk03"
msgstr ""
"[Sound1 bytes=\"0002\"]Ich gehe ins Bett.[PauseShort] Aber der Laden hat\n"
"selbstverständlich trotzdem noch auf!"

msgid "talk16"
msgstr ""
"[Sound1 bytes=\"0414\"]Ah...[PauseShort] eine [Color name=\"blue\"]blaue Flamme[Color name=\"unset\"]![PauseLong]\n"
"Die Leute vom [Color name=\"blue\"]Institut [Color name=\"unset\"]haben oft so\n"
"eine herumgetragen."

msgid "talk04"
msgstr ""
"[Sound1 bytes=\"0413\"]Ah...[PauseShort] eine [Color name=\"blue\"]blaue Flamme[Color name=\"unset\"]![PauseLong]\n"
"Die alte Purah hat oft so eine\n"
"herumgetragen."

msgid "talk17"
msgstr ""
"[Sound1 bytes=\"0019\"]Ja! Ich kenne diese [Color name=\"blue\"]Flamme[Color name=\"unset\"]![PauseLong]\n"
"So eine hat die alte Purah oft\n"
"herumgetragen!"

msgid "talk05"
msgstr ""
"[Sound1 bytes=\"0301\"]Ein bescheidenes Wetter haben\n"
"wir heute..."

msgid "talk18"
msgstr ""
"[Sound1 bytes=\"0019\"]Ja! Ich kenne diese [Color name=\"blue\"]Flamme[Color name=\"unset\"]![PauseLong]\n"
"So eine haben die Leute vom [Color name=\"blue\"]Institut[Color name=\"unset\"]\n"
"oft herumgetragen!"

msgid "talk06"
msgstr ""
"[Sound1 bytes=\"0009\"]Der Krämerladen [Color name=\"blue\"]Mistral[Color name=\"unset\"][PauseShort] wartet\n"
"auf deinen Besuch!"

msgid "talk19"
msgstr ""
"[Sound1 bytes=\"0002\"]Ich gehe jetzt ins Bett![PauseLong]\n"
"Wenn du mit mir reden möchtest,\n"
"komm wann anders wieder."

msgid "talk07"
msgstr ""
"[Sound1 bytes=\"0009\"]Aber der Krämerladen [Color name=\"blue\"]Mistral [Color name=\"unset\"]ist\n"
"natürlich trotzdem für dich da!"

msgid "talk08"
msgstr "[Sound1 bytes=\"0001\"]Sieh dich in aller Ruhe um!"

msgid "talk09"
msgstr "[Sound1 bytes=\"0001\"]Schönes Wetter, oder?"

