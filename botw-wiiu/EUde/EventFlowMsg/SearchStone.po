msgid "Npc_SouthernVillage014_R_001"
msgstr "[Sound1 unk1=\"11\" unk2=\"16\"]Hmm...[PauseLong][PageBreak]...[PauseMid] ..."

msgid "Npc_SouthernVillage014_R_002"
msgstr ""
"[Sound1 unk1=\"10\" unk2=\"21\"]Wah![PauseLong][PageBreak][Sound1 unk1=\"0\" unk2=\"1\"]Oh, tut mir leid.[PauseShort]\n"
"Ich war ganz in Gedanken versunken.[PauseLong]\n"
"Kann ich dir mit etwas helfen?[Choice2 label1=\"0\" label2=\"1\" selectednum=\"0\" cancelnum=\"1\"]"

msgid "Npc_SouthernVillage014_R_003"
msgstr ""
"[Sound2 id=\"2\"]Ah, ich war gerade damit beschäftigt,\n"
"diese Steintafel zu untersuchen."

msgid "Npc_SouthernVillage014_R_004"
msgstr "[Size percent=\"80\"]Ganz schön kurz angebunden...[Size percent=\"100\"]"

msgid "Npc_SouthernVillage014_R_005"
msgstr ""
"[Sound2 id=\"23\"]Oh, du bist also genauso wissbegierig\n"
"wie ich?"

