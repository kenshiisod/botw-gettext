msgstr "[Color name=\"blue\"]Urbosas Tagebuch[Color name=\"unset\"] lesen?[Choice3 label1=\"5\" label2=\"6\" label3=\"1\" select_idx=\"0\" cancel_idx=\"2\"]"

msgid "0002"
msgstr "Nächste Seite"

