msgid "talk09"
msgstr "[Sound1 bytes=\"0001\"][Size percent=\"125\"]GAAH-PFOOOH...[Size percent=\"100\"]"

msgid "talk10"
msgstr "[Sound1 bytes=\"0001\"][Size percent=\"125\"]GRRRRRR-PÜÜÜÜ...[Size percent=\"100\"]"

msgid "talk11"
msgstr "[Sound1 bytes=\"0106\"]Kann ich dir irgendwie helfen?[Choice4 label1=\"0\" label2=\"1\" label3=\"2\" label4=\"3\" select_idx=\"0\" cancel_idx=\"3\"]"

msgid "talk12"
msgstr "[Sound1 bytes=\"0004\"]Bis dann!"

msgid "talk13"
msgstr "[Sound1 bytes=\"0013\"]Nett, dass du fragst..."

msgid "TerrorBomb"
msgstr "Wa-wa-wa-waaas?!"

msgid "talk14"
msgstr ""
"[Sound1 bytes=\"0004\"]Seit [Color name=\"blue\"]Rudania[Color name=\"unset\"] sich dort niedergelassen\n"
"hat, gibt er keinen Mucks mehr von sich.[PageBreak]Wenn ich mir vorstelle, dass wir gegen\n"
"dieses Monstrum gekämpft haben, wird\n"
"mir immer noch ganz anders..."

