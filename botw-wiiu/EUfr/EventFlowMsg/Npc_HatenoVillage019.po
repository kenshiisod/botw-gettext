msgstr "Ça fait [Color name=\"red\"][MoneyVar name=\"ShopTradePrice\"] rubis [Color name=\"unset\"]les [02:0F name=\"Shop_TradeItemNum\"]. Ça te va ?[Choice2 label1=\"5500\" label2=\"5501\" selectednum=\"0\" cancelnum=\"1\"]"

msgid "talk10"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Merci d'avoir choisi Vent d'est !"

msgid "Com_Talk_15"
msgstr "[Sound2 id=\"14\"]Désolée, mais je n'ai pas besoin de ça."

msgid "talk11"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Je te remercie !"

msgid "Com_Talk_16"
msgstr ""
"[Sound2 id=\"12\"]Ah non, je n'achète jamais un article\n"
