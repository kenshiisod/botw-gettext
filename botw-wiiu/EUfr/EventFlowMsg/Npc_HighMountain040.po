msgstr "[Sound1 bytes=\"0001\"]Tu veux bien m'en vendre [Color name=\"blue\"]cinq [Color name=\"unset\"]?[Choice2 label1=\"3\" label2=\"4\" select_idx=\"0\" cancel_idx=\"1\"]"

msgid "0003"
msgstr "D'accord !"

msgid "talk22"
msgstr "[Sound1 bytes=\"0001\"]Tu veux bien m'en vendre [Color name=\"blue\"]dix [Color name=\"unset\"]?[Choice2 label1=\"3\" label2=\"4\" select_idx=\"0\" cancel_idx=\"1\"]"

msgid "0004"
msgstr "Pas question."

msgid "talk10"
msgstr "[Sound1 bytes=\"0000\"]C'est ça, à plus tard !"

