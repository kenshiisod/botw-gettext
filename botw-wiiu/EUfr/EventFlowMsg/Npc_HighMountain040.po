msgstr "[Sound1 bytes=\"0001\"]Tu veux bien m'en vendre [Color name=\"blue\"]30 [Color name=\"unset\"]?[Choice2 label1=\"3\" label2=\"4\" select_idx=\"0\" cancel_idx=\"1\"]"

msgid "talk11"
msgstr ""
"[Sound1 bytes=\"0109\"]Hé ![PauseShort] T'as l'air d'avoir la pêche, malgré\n"
"ce froid !"

msgid "talk24"
msgstr "[Sound1 bytes=\"0001\"]Tu veux bien m'en vendre [Color name=\"blue\"]50 [Color name=\"unset\"]?[Choice2 label1=\"3\" label2=\"4\" select_idx=\"0\" cancel_idx=\"1\"]"

