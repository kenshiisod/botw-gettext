msgstr "Enregistrer un cheval."

msgid "0004"
msgstr "Jusqu'au matin."

msgid "0065"
msgstr "Non, rien !"

msgid "0005"
msgstr "Jusqu'à midi."

msgid "0066"
msgstr "S'il te plaît !"

msgid "NakedPlayer"
msgstr "T'as pas froid ?"

