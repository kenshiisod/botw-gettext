msgid "talk07"
msgstr ""
"[Sound1 unk1=\"6\" unk2=\"0\"]Goedemorgen...[PauseLong] Ik ben nog een beetje\n"
"slaperig, maar ik heb altijd tijd voor\n"
"vragen van mijn leerlingen."

msgid "talk08"
msgstr ""
"[Sound1 unk1=\"6\" unk2=\"0\"]Vandaag is het ook weer heet...[PauseLong] Luister\n"
"goed, leerling."

msgid "talk09"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"34\"]Ik ben best slaperig, maar ik zal de\n"
"vragen van mijn leerling beantwoorden."

msgid "0000"
msgstr "Ik ben een reiziger."

msgid "0001"
msgstr "Tot ziens!"

msgid "talk20"
msgstr "[Sound1 unk1=\"0\" unk2=\"4\"]Ik heb geen problemen."

msgid "0002"
msgstr "Wie ben jij?"

msgid "talk21"
msgstr ""
"[Sound1 unk1=\"11\" unk2=\"1\"]Als je iemand met een probleem zoekt...[PauseShort]\n"
"Baas Bludo zit in de penarie vanwege zijn\n"
"gammele rug..."

msgid "0003"
msgstr "Wat is dit voor plek?"

msgid "talk22"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]De rug van baas Bludo is weer in orde.[PauseShort]\n"
"Alles is weer vredig."

msgid "0004"
msgstr "Waar woon je?"

msgid "talk10"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Heb je vragen voor me?[Choice3 label1=\"5\" label2=\"6\" label3=\"1\" selectednum=\"0\" cancelnum=\"2\"]"

msgid "0005"
msgstr "Is er iets mis?"

msgid "talk11"
msgstr "[Sound1 unk1=\"6\" unk2=\"0\"]Hmm..."

msgid "talk24"
msgstr "[Sound1 unk1=\"3\" unk2=\"34\"]Ik wilde net naar bed gaan..."

msgid "0006"
msgstr "Wat doe je?"

msgid "talk12"
msgstr "[Sound1 unk1=\"3\" unk2=\"12\"]Ik ben op weg naar huis..."

msgid "talk25"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Verder nog iets?[Choice3 label1=\"5\" label2=\"6\" label3=\"1\" selectednum=\"0\" cancelnum=\"2\"]"

