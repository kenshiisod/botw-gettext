msgid "0001"
msgstr "Нет"

msgid "talk_09"
msgstr "[Sound1 unk1=\"0\" unk2=\"12\"]Ну ладно..."

msgid "0002"
msgstr "Что это за место?"

msgid "0003"
msgstr "Пока"

msgid "talk_00"
msgstr "[Sound1 unk1=\"7\" unk2=\"0\"]Доброе утро![PauseShort] Ты постоялец?[Choice4 label1=\"0\" label2=\"1\" label3=\"2\" label4=\"3\" selectednum=\"0\" cancelnum=\"3\"]"

msgid "talk_10"
msgstr ""
"[Sound1 unk1=\"1\" unk2=\"4\"]Это [Color name=\"blue\"]конюшня «Рито»[Color name=\"unset\"].[PageBreak][Sound1 unk1=\"0\" unk2=\"0\"]Обратись на стойку снаружи, если\n"
"хочешь зарегистрировать лошадь.[PageBreak]Если нужно переночевать,\n"
"подойди к стойке внутри.[PageBreak]Тебе, наверное, интересно, почему\n"
"эта конюшня так называется..."

msgid "talk_01"
msgstr "[Sound1 unk1=\"7\" unk2=\"0\"]Привет![PauseShort] Ты постоялец?[Choice4 label1=\"0\" label2=\"1\" label3=\"2\" label4=\"3\" selectednum=\"0\" cancelnum=\"3\"]"

msgid "talk_11"
msgstr "[Sound1 unk1=\"0\" unk2=\"12\"]Ну ладно.[PauseShort] До свидания."

msgid "talk_02"
msgstr "[Sound1 unk1=\"4\" unk2=\"11\"]Добрый вечер![PauseShort] Ты постоялец?[Choice4 label1=\"0\" label2=\"1\" label3=\"2\" label4=\"3\" selectednum=\"0\" cancelnum=\"3\"]"

msgid "talk_12"
msgstr ""
"[Sound1 unk1=\"1\" unk2=\"9\"]Там живет [Color name=\"blue\"]народ рито[Color name=\"unset\"].[PauseShort] Поэтому\n"
"и конюшня так называется."

msgid "talk_13"
msgstr ""
"[Sound1 unk1=\"7\" unk2=\"0\"]Доброе утро![PauseShort] Похоже, погода\n"
"сегодня будет хорошая!"

msgid "talk_04"
msgstr ""
"[Sound1 unk1=\"1\" unk2=\"4\"]Если тебе что-нибудь понадобится,\n"
"обращайся к хозяину конюшни — он\n"
"за стойкой."

msgid "talk_14"
msgstr ""
"[Sound1 unk1=\"6\" unk2=\"0\"]Доброе утро.[PauseShort] Похоже, погода\n"
"целый день будет плохая..."

msgid "talk_15"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Видишь ту высокую скалу?"

msgid "talk_16"
msgstr ""
"[Sound1 unk1=\"1\" unk2=\"4\"]Это [Color name=\"blue\"]конюшня «Рито»[Color name=\"unset\"].[PageBreak]Обратись на стойку снаружи, если\n"
"хочешь зарегистрировать лошадь.[PageBreak]Если нужно переночевать,\n"
"подойди к стойке внутри.[PageBreak]А свое название конюшня получила\n"
"в честь [Color name=\"blue\"]народа рито[Color name=\"unset\"], чья деревня\n"
"находится к западу отсюда."

msgid "0000"
msgstr "Точно!"

msgid "talk_08"
msgstr "[Sound1 unk1=\"7\" unk2=\"4\"]Ты постоялец, да?[Choice4 label1=\"0\" label2=\"1\" label3=\"2\" label4=\"3\" selectednum=\"0\" cancelnum=\"3\"]"

