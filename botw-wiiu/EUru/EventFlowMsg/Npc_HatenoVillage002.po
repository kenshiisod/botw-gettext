msgid "talk23"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"4\"]У нас самообслуживание: встань\n"
"перед нужным товаром и возьми\n"
"его."

msgid "0001"
msgstr "Хочу кое-что купить"

msgid "0002"
msgstr "Я кое-что продаю"

msgid "0003"
msgstr "Пока"

msgid "0004"
msgstr "Кто ты?"

