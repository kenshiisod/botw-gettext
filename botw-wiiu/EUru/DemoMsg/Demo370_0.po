msgid "Demo370_0_Text008"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"6\"]Погодите...[PauseLong] Странный\n"
"какой-то у нас священник."

msgid "Demo370_0_Text018"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]Прошу всех присутствующих\n"
"поздравить новобрачных!"

msgid "Demo370_0_Text009"
msgstr "[Sound1 unk1=\"0\" unk2=\"6\"]Думаешь?"

msgid "Demo370_0_Text019"
msgstr ""
"[Sound1 unk1=\"7\" unk2=\"1\"]Уважаемый жених Топорда![PauseShort]\n"
"Уважаемая невеста Герда![PauseShort]\n"
"Сегодня вы вступаете в брак!"

msgid "Demo370_0_Text000"
msgstr "[Sound2 id=\"7\"][Sound1 unk1=\"0\" unk2=\"1\"]Кхе-кхе!"

msgid "Demo370_0_Text010"
msgstr "[Sound1 unk1=\"0\" unk2=\"11\"]Если хочешь, могу поклясться за тебя."

msgid "Demo370_0_Text020"
msgstr "..."

msgid "Demo370_0_Text001"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Жених Топорда!"

msgid "Demo370_0_Text011"
msgstr "[Sound1 unk1=\"10\" unk2=\"0\"]Начальник, ты что, шутишь?"

msgid "Demo370_0_Text021"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]Дорогие супруги! Пусть ваша любовь\n"
"будет светлой и чистой, как весеннее\n"
"небо![PauseShort] Объявляю вас мужем и женой!"

msgid "Demo370_0_Text002"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]Берешь ли ты Герду в законные жены?[PauseShort]\n"
"Клянешься ли любить и оберегать ее\n"
"и в радости, и в печали?"

msgid "Demo370_0_Text012"
msgstr "[Sound1 unk1=\"7\" unk2=\"1\"][Animation name=\"LaughL_00\"]Сражаться открыто и честно!"

msgid "Demo370_0_Text022"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Во имя богини Хайлии..."

msgid "Demo370_0_Text003"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]Берешь ли ты Топорду\n"
"в законные мужья?"

msgid "Demo370_0_Text013"
msgstr "[Sound1 unk1=\"7\" unk2=\"9\"]И по правилам!"

msgid "Demo370_0_Text023"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Прошу тишины.[PageBreak]Э-э...[PageBreak]Хм..."

msgid "Demo370_0_Text004"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Клянусь."

msgid "Demo370_0_Text014"
msgstr "[Size percent=\"125\"][Sound1 unk1=\"7\" unk2=\"10\"]Клянусь![Size percent=\"100\"]"

msgid "Demo370_0_Text005"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Хорошо."

msgid "Demo370_0_Text015"
msgstr "[Sound2 id=\"10\"][Sound1 unk1=\"0\" unk2=\"1\"]Клянусь..."

msgid "Demo370_0_Text006"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Невеста Герда!"

msgid "Demo370_0_Text016"
msgstr "[Sound1 unk1=\"7\" unk2=\"10\"]А-ха-ха-ха!"

msgid "Demo370_0_Text007"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]Клянешься ли ты всегда сражаться\n"
"открыто, честно и по правилам...[PauseShort]\n"
"и в радости, и в печали?"

msgid "Demo370_0_Text017"
msgstr "[Size percent=\"125\"][Sound2 id=\"12\"][Sound1 unk1=\"0\" unk2=\"1\"]Кхе-кхе![Size percent=\"100\"]"

