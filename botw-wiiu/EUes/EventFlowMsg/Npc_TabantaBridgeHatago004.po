msgid "talk04"
msgstr ""
"[Sound1 bytes=\"0700\"]¿Y sabes qué?[PauseShort] Le pedí que me pintase\n"
"un cuadro de un bichito.[PauseMid] ¡Solo para mí!"

msgid "talk05"
msgstr ""
"[Sound1 bytes=\"0701\"]El [Color name=\"blue\"]señor mayor que pinta[Color name=\"unset\"][PauseMid] me hizo\n"
"un dibujito de un bicho,[PauseMid] ¡para mí solito!"

msgid "talk40"
msgstr ""
"[Sound1 bytes=\"0100\"]Estoy esperando a que venga\n"
"el señor de los bichitos...\n"
"¡Seguro que no tarda!"

msgid "talk70"
msgstr "[Sound1 bytes=\"0000\"]Me gustaría salir, pero está lloviendo."

msgid "talk71"
msgstr ""
"[Sound1 bytes=\"0701\"]Hay bichitos que solo salen...[PauseMid] cuando\n"
"llueve...[PauseMid] Como la [Color name=\"blue\"][C9:00 bytes=\"02010100\"]mariposa electro[Color name=\"unset\"]..."

msgid "talk50"
msgstr ""
"[Sound1 bytes=\"0700\"]Es un señor que lleva un bicho\n"
"enooorme[PauseShort] a la espalda.[PauseMid]\n"
"¡Yo también quiero!"

msgid "talk00"
msgstr "[Sound1 bytes=\"0004\"]No hay ningún bichito..."

msgid "talk01"
msgstr ""
"[Sound1 bytes=\"0000\"]¿Dónde se habrán metido\n"
"los bichitos raros?[PauseMid] Jo..."

msgid "talk02"
msgstr "[Sound1 bytes=\"0000\"]¡Hala![PauseShort] ¡Tú por aquí!"

msgid "talk60"
msgstr "[Size percent=\"80\"][Sound2 id=\"31\"]Bichiiitos...[PauseShort] ¡por todas partes![Size percent=\"100\"]"

msgid "talk03"
msgstr ""
"[Sound1 bytes=\"0000\"]El [Color name=\"blue\"]señor mayor que pinta[Color name=\"unset\"][PauseShort] se marchó\n"
"otra vez.[PauseMid] ¡Pero me pidió que te\n"
"saludara de su parte!"

msgid "talk55"
msgstr ""
"[Sound1 bytes=\"0701\"]Jo, yo también quiero tener una mochila.[PauseShort]\n"
"[Sound1 bytes=\"0001\"]A lo mejor puedo comprarme una\n"
"cuando sea mayor..."

msgid "talk61"
msgstr ""
"[Sound1 bytes=\"0900\"]Hoy no he visto muchos bichos raros...[PauseMid]\n"
"¡Qué rabia!"

