msgid "Talk_01"
msgstr "[Sound1 unk1=\"7\" unk2=\"2\"]¡Buenas![Choice3 label1=\"1\" label2=\"2\" label3=\"3\" selectednum=\"0\" cancelnum=\"2\"]"

msgid "Talk_14"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]Por ahí queda la [Color name=\"blue\"]zona de entrenamiento[Color name=\"unset\"].[PageBreak][Sound1 unk1=\"1\" unk2=\"9\"]Espero que [Color name=\"blue\"]mi padre[Color name=\"unset\"] no tarde mucho.[PauseShort]\n"
"¡Qué ganas!"

msgid "Talk_02"
msgstr "[Sound1 unk1=\"7\" unk2=\"2\"]¡Buenas noches![Choice3 label1=\"2\" label2=\"0\" label3=\"3\" selectednum=\"0\" cancelnum=\"2\"]"

msgid "Talk_03"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"10\"]Sí, señor, ¡así se responde![PageBreak]Como dice [Color name=\"blue\"]mi padre[Color name=\"unset\"]:\n"
"«Todo muchacho decente\n"
"saluda apropiadamente»."

msgid "Talk_16"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]Como ya está mucho mejor,\n"
"me ha dicho que me llevará a la\n"
"[Color name=\"blue\"]zona de entrenamiento[Color name=\"unset\"], ¿sabes?"

msgid "Talk_04"
msgstr ""
"[Sound1 unk1=\"4\" unk2=\"20\"]¿Cómo?[PauseShort] ¿Se supone que eso es\n"
"un saludo?"

msgid "Talk_17"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]¡[Color name=\"blue\"]Mi padre[Color name=\"unset\"] ha prometido llevarme a\n"
"la [Color name=\"blue\"]zona de entrenamiento[Color name=\"unset\"]! Ya se ha\n"
"recuperado bastante de sus heridas."

msgid "Talk_05"
msgstr "[Sound1 unk1=\"0\" unk2=\"2\"]¿Qué passsa?[PauseShort] Ah, eres tú."

msgid "Talk_18"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"1\"]¿Te he contado que he quedado en ir\n"
"a la [Color name=\"blue\"]zona de entrenamiento[Color name=\"unset\"] con\n"
"[Color name=\"blue\"]mi padre[Color name=\"unset\"] cuando vuelva?[Choice2 label1=\"7\" label2=\"6\" selectednum=\"0\" cancelnum=\"1\"]"

msgid "Talk_06"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"2\"]¿Qué passsa,[PauseShort] chico de los saludos\n"
"originales?"

msgid "Talk_19"
msgstr ""
"[Sound1 unk1=\"1\" unk2=\"1\"]Vaya...[PauseShort] [Color name=\"blue\"]Mi padre[Color name=\"unset\"] ha salido por fin\n"
"del trabajo.[PageBreak][Sound1 unk1=\"3\" unk2=\"1\"]Pero por lo visto ha tenido\n"
"un accidente laboral..."

msgid "Talk_07"
msgstr "[Sound1 unk1=\"7\" unk2=\"0\"]¡Qué ganas tengo de ir!"

msgid "Talk_08"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"1\"]¡Adivina qué![PauseShort] ¡[Color name=\"blue\"]Mi padre[Color name=\"unset\"] me ha prometido\n"
"llevarme a entrenar luego![Choice2 label1=\"5\" label2=\"6\" selectednum=\"0\" cancelnum=\"2\"]"

