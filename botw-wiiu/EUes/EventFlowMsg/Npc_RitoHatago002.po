msgid "talk_00"
msgstr "[Sound1 bytes=\"0700\"]Muy buenos días. [PauseMid]¿Buscas alojamiento?[Choice4 label1=\"0\" label2=\"1\" label3=\"2\" label4=\"3\" select_idx=\"0\" cancel_idx=\"3\"]"

msgid "talk_10"
msgstr ""
"[Sound1 bytes=\"0104\"]Esta es la [Color name=\"blue\"]posta orni[Color name=\"unset\"].[PageBreak][Sound1 bytes=\"0000\"]Podemos registrar tus monturas\n"
"desde el mostrador de la entrada.[PageBreak]Y, si accedes al interior, podrás\n"
"pedir una cama para descansar.[PageBreak]Si te preguntas de dónde viene\n"
"eso de [Color name=\"blue\"]orni[Color name=\"unset\"]..."

msgid "talk_01"
msgstr "[Sound1 bytes=\"0700\"]Hola.[PauseMid] ¿Buscas alojamiento?[Choice4 label1=\"0\" label2=\"1\" label3=\"2\" label4=\"3\" select_idx=\"0\" cancel_idx=\"3\"]"

