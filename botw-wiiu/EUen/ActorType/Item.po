msgstr "Silver Rupee"

msgid "Item_Chilled_02_Name"
msgstr "Icy Prime Meat"

msgid "Item_Boiled_01_Name"
msgstr "Hard-Boiled Egg"

msgid "Item_Enemy_20_Name"
msgstr "Octorok Tentacle"

