msgid "Npc_SouthernVillage014_R_001"
msgstr "[Sound1 unk1=\"11\" unk2=\"16\"]Mmmh...[PauseLong][PageBreak]...[PauseMid] ...[PauseLong]"

msgid "Npc_SouthernVillage014_R_002"
msgstr ""
"[Sound1 unk1=\"10\" unk2=\"21\"]Ooops.[PauseLong][PageBreak][Sound1 unk1=\"0\" unk2=\"1\"]Scusami!\n"
