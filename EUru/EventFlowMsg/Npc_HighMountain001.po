msgid "Talk_01"
msgstr "[Sound1 unk1=\"7\" unk2=\"2\"]Добрый день![Choice3 label1=\"1\" label2=\"2\" label3=\"3\" selectednum=\"0\" cancelnum=\"2\"]"

msgid "Talk_14"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]Вон то место с флагами и есть\n"
"[Color name=\"blue\"]летная школа[Color name=\"unset\"]![PageBreak][Sound1 unk1=\"1\" unk2=\"9\"]Хоть бы [Color name=\"blue\"]папа[Color name=\"unset\"] скорее вернулся![PauseShort]\n"
"Мне не терпится там поиграть!"

msgid "Talk_02"
msgstr "[Sound1 unk1=\"7\" unk2=\"2\"]Добрый вечер![Choice3 label1=\"2\" label2=\"0\" label3=\"3\" selectednum=\"0\" cancelnum=\"2\"]"

msgid "Talk_03"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"10\"]Хороший ответ![PageBreak][Color name=\"blue\"]Папа[Color name=\"unset\"] говорит, что очень важно\n"
"уметь правильно здороваться!"

msgid "Talk_16"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]Но ему уже намного лучше.[PauseShort] Скоро он\n"
"снова возьмет меня в [Color name=\"blue\"]летную школу[Color name=\"unset\"]!"

msgid "Talk_04"
msgstr "[Sound1 unk1=\"4\" unk2=\"20\"]Дядя, ты очень странный."

msgid "Talk_17"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]Кажется, [Color name=\"blue\"]папина[Color name=\"unset\"] рана почти зажила.[PageBreak]Он пообещал, что скоро мы снова\n"
"пойдем играть у [Color name=\"blue\"]летной школы[Color name=\"unset\"]!"

msgid "Talk_05"
msgstr "[Sound1 unk1=\"0\" unk2=\"2\"]Привет!"

msgid "Talk_18"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"1\"][Color name=\"blue\"]Папа[Color name=\"unset\"] обещал поиграть со мной\n"
"у [Color name=\"blue\"]летной школы[Color name=\"unset\"], когда вернется![Choice2 label1=\"7\" label2=\"6\" selectednum=\"0\" cancelnum=\"1\"]"

msgid "Talk_06"
msgstr "[Sound1 unk1=\"0\" unk2=\"2\"]Привет, странный дядя!"

msgid "Talk_19"
msgstr "[Sound1 unk1=\"1\" unk2=\"1\"][Color name=\"blue\"]Папа[Color name=\"unset\"] наконец-то вернулся с работы![PageBreak][Sound1 unk1=\"3\" unk2=\"1\"]Он, кажется, поранился..."

msgid "Talk_07"
msgstr "[Sound1 unk1=\"7\" unk2=\"0\"]Жду не дождусь!"

msgid "Talk_08"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"1\"]А знаешь что?[PauseShort] [Color name=\"blue\"]Папа[Color name=\"unset\"] обещал\n"
"поиграть со мной![Choice2 label1=\"5\" label2=\"6\" selectednum=\"0\" cancelnum=\"2\"]"

msgid "0000"
msgstr "Доброе утро!"

msgid "0001"
msgstr "Добрый день!"

msgid "0002"
msgstr "Добрый вечер!"

msgid "0003"
msgstr "Пока"

msgid "0005"
msgstr "Класс!"

msgid "0006"
msgstr "Ого!"

msgid "0007"
msgstr "Где летная школа?"

msgid "Talk_20"
msgstr ""
"[Sound1 unk1=\"1\" unk2=\"26\"]Скоро!♪[PauseShort] Очень скоро!♪[PauseShort]\n"
"[Color name=\"blue\"]Папа[Color name=\"unset\"] вернется очень скоро!♪"

msgid "Talk_21"
msgstr "[Sound1 unk1=\"1\" unk2=\"26\"]Ля-ля-ля![PauseShort] М-м-м!♪"

msgid "Talk_22"
msgstr "[Sound1 unk1=\"1\" unk2=\"1\"]Ты тоже приходи!"

msgid "Talk_10"
msgstr "[Sound1 unk1=\"0\" unk2=\"33\"]Фью...[PauseMid] [Color name=\"blue\"]Папа[Color name=\"unset\"]?.."

msgid "Talk_23"
msgstr "[Sound1 unk1=\"7\" unk2=\"0\"]Честно-честно?[PauseShort] Ура!"

msgid "Talk_11"
msgstr "[Sound1 unk1=\"9\" unk2=\"34\"]Уо-о-а...[PauseMid] Спать..."

msgid "Talk_24"
msgstr "[Sound1 unk1=\"7\" unk2=\"0\"]Обязательно приходи — поиграем!"

msgid "Talk_12"
msgstr ""
"[Sound1 unk1=\"1\" unk2=\"1\"]Да![PauseShort] У [Color name=\"blue\"]летной школы[Color name=\"unset\"] можно\n"
"расправить крылья и вух! —\n"
"взмыть в небо![Choice2 label1=\"7\" label2=\"6\" selectednum=\"0\" cancelnum=\"1\"]"

msgid "Talk_00"
msgstr "[Sound1 unk1=\"7\" unk2=\"2\"]Доброе утро![Choice3 label1=\"0\" label2=\"2\" label3=\"3\" selectednum=\"0\" cancelnum=\"2\"]"

msgid "Talk_13"
msgstr ""
"[Sound1 unk1=\"10\" unk2=\"0\"]Почему ты так ходишь?[PauseShort]\n"
"Тебе не холодно?"

