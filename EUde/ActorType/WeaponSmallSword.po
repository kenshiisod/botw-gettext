msgid "Weapon_Sword_053_Name"
msgstr "[C9:00 bytes=\"01000000\"]Kopfsammler"

msgid "Weapon_Sword_015_Name"
msgstr "[C9:00 bytes=\"03020200\"]Wächter-Schwert ++"

