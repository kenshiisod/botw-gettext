msgid "Com_Talk_02"
msgstr "[Sound1 unk1=\"0\" unk2=\"9\"]Dank-Dank!"

msgid "0001"
msgstr "Kaufst du an?"

msgid "0002"
msgstr "Wiedersehen."

msgid "Com_Talk_04"
msgstr "[Sound1 unk1=\"11\" unk2=\"0\"]Was? Du willst nicht mehr?"

msgid "Com_Talk_05"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]War es das schon?\n"
"Ich kaufe alles gern an!"

msgid "Com_Talk_05b"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Verkauf ganz viel!"

msgid "0005"
msgstr "Fleisch!"

msgid "Com_Talk_05c"
msgstr "[Sound1 unk1=\"0\" unk2=\"0\"]Ich kaufe, ich kaufe!"

msgid "Talk_01"
msgstr "[Sound1 unk1=\"1\" unk2=\"2\"]Guten Morq![PageBreak]Was isst du denn so zum Frühstück?[Choice2 label1=\"5\" label2=\"6\" selectednum=\"0\" cancelnum=\"1\"]"

msgid "0006"
msgstr "Gemüse!"

msgid "Com_Talk_05d"
msgstr ""
"[Sound1 unk1=\"0\" unk2=\"0\"]Nichts mehr, was du noch verkaufen\n"
"willst? Guck ganz genau in deine\n"
"Taschen!"

msgid "Talk_20"
msgstr "[Sound1 unk1=\"1\" unk2=\"9\"]Guten Morq!"

msgid "Talk_03"
msgstr ""
"[Sound1 unk1=\"1\" unk2=\"9\"]Dank-Dank![PauseShort]\n"
"Auf Wiedervorq!"

